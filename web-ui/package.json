{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ../assets/web-ui-public-build/public/*",
    "clean-maps": "rimraf ../assets/web-ui-public-build/public/**/*.map",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dist": "npm run clean && npm run build && copyfiles --all --verbose ./public/**/* ../assets/web-ui-public-build/ && npm run clean-maps"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}
